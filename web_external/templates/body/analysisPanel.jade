.panelTitle(data-toggle='collapse', data-target='.m-analysisPanelContainer')
  | Analyses
  i(class='icon-up-open close-icon toggle-icon')
.m-analysisPanelContainer.collapse.in
  .m-analysisList
    .m-analyses
        each analysis in analyses
            if analysis.getMinervaMetadata()
                .m-analysis= analysis.get('name')
                    - var attributes = {'m-analysis-id': analysis.get('_id')}
                    - var classes = 'icon-play-circled m-attempt-analysis'
                    i(class=classes)&attributes(attributes)
