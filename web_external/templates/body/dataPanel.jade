.dataPanelContainer
  .panelTitle
    | Datasets
  .loadData
     button.btn.btn-default.m-add-dataset-button(title="Upload dataset")
       .icon-plus-squared
         | add dataset
  .savedDatasets
    .savedDatasetsTitle
    .savedDataList
      ul.datasets
        each dataset in datasets
          li.dataset= dataset.get('name')
            if (dataset.get('inLayers'))
                i.icon-right-circled.dataset-in-layers&attributes({'m-dataset-id': dataset.get('_id')})
                i.icon-trash.delete-dataset.dataset-in-layers&attributes({'m-dataset-id': dataset.get('_id')})
            else
                i.icon-right-circled.add-dataset-to-layers&attributes({'m-dataset-id': dataset.get('_id')})
                i.icon-trash.delete-dataset&attributes({'m-dataset-id': dataset.get('_id')})
