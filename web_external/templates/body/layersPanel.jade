.layersPanelContainer
  .panelTitle
    | Session Layers
  .layersList
    ul.datasets
      each dataset in datasets
        li.dataset= dataset.get('name')
          i.icon-cancel-circled.remove-dataset-from-layer&attributes({'m-dataset-id': dataset.get('_id')})
          div.opacity-contanier
            input.opacity-range&attributes({'m-dataset-id': dataset.get('_id')})(type='range', min=0, max=1, step=0.01, value=dataset.get('opacity'))
