.dataPanelContainer
    text-align center

    .loadData

        button
            margin auto

    .savedDatasets
        margin-top 20px

        .savedDatasetsTitle
            border-bottom 1px solid #e6e6e6
            margin-bottom 20px

        .savedDataList

            ul
                list-style-type none
                padding-left 0px
                text-align left

                li
                    border 0
                    margin 2px
                    padding 2px

            .icon-right-circled
                float left

            .dataset-in-layers
                color #f0f0f0

            .add-dataset-to-layers
                color #444

            .add-dataset-to-layers:hover
                color #66a
                cursor pointer 
