.m-session-header
  border-bottom 1px solid #ddd
  margin-bottom 10px
  padding-bottom 5px
  min-height 40px

  .m-session-title
    font-weight bold
    font-size 21px
    color #222

  .m-session-description
    color #666

  .m-session-header-buttons
    float right

  .m-save-session-button
    margin-right 10px

.gridster

    > ul > li
    
        border 0px
        padding 0px
        margin 5px
        list-style-type none
        background-color #f8f8f8

.panelTitle
    font-weight bold
    text-align center
    background-color #cfcfcf
    margin-bottom 20px
    cursor move
    padding 10px 0px
